{
  "name": "blog-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext \".js,.vue,.ts\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lint-fix": "eslint --fix --ext .ts --ext .js --ext .jsx --ext .vue src/"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "^1.11.1",
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/pwa": "^3.3.5",
    "@toast-ui/editor": "^2.5.1",
    "@toast-ui/editor-plugin-chart": "^1.0.0",
    "@toast-ui/editor-plugin-code-syntax-highlight": "^1.0.0",
    "@toast-ui/editor-plugin-color-syntax": "^1.0.1",
    "@toast-ui/editor-plugin-table-merged-cell": "^1.3.2",
    "@toast-ui/editor-plugin-uml": "^1.0.0",
    "@toast-ui/vue-editor": "^2.5.1",
    "core-js": "^3.8.3",
    "cross-env": "^5.2.0",
    "crypto-js": "^3.3.0",
    "element-ui": "^2.15.0",
    "html2canvas": "^1.0.0-rc.7",
    "jspdf": "^2.3.0",
    "lodash": "^4.17.21",
    "markdown": "^0.5.0",
    "nuxt": "^2.14.12",
    "qs": "^6.9.6",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vuex-module-decorators": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.15.1",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/crypto-js": "^4.0.1",
    "@types/lodash": "^4.14.168",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "fix": "^0.0.6",
    "husky": "^4.3.8",
    "less": "^4.1.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.5.3",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "problem": "^0.0.0",
    "sass-loader": "^11.0.1",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0"
  }
